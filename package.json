{
  "name": "mochapacks",
  "version": "0.0.1",
  "description": "A new reporter focusing on suites & testpacks",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npx babel src --out-dir lib",
    "dev": "npx babel src --watch --out-dir lib",
    "test": "npx mocha",
    "format": "prettier --write --tab-width 4 \"src/**/*.js\" \"test/**/*.js\"",
    "lint": "npx eslint src",
    "lint!": "npm run format && npm run lint",
    "precommit": "lint-staged",
    "commit": "npx git-cz"
  },
  "lint-staged": {
    "src/*.js": [
      "npm run lint!",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eonuk/mochapacks.git"
  },
  "author": "Ian Webb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eonuk/mochapacks/issues"
  },
  "homepage": "https://github.com/eonuk/mochapacks#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3"
  }
}
